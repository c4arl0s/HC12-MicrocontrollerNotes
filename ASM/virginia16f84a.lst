0082   008C           00220                 movwf    0x0c                           ;cargo la direccion con 0ch con el w.
0083   008D           00221         movwf    0x0d                           ;cargo la direccion con 0dh con el w.
0084   3001           00222                 movlw    0x01                           ; cargo w con el dato 01h. 
